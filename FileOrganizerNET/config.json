{
  "Rules": [
    {
      "DestinationFolder": "Archive/Old Files",
      "Conditions": {
        "olderThanDays": 365
      }
    },
    {
      "DestinationFolder": "Large Files",
      "Conditions": {
        "minSizeMB": 5120
      }
    },
    {
      "DestinationFolder": "Invoices",
      "Conditions": {
        "fileNameContains": [
          "invoice",
          "receipt"
        ],
        "extensions": [
          ".pdf",
          ".docx",
          ".xlsx"
        ]
      }
    },
    {
      "DestinationFolder": "Photos",
      "Conditions": {
        "extensions": [
          ".jpg",
          ".jpeg",
          ".png",
          ".gif",
          ".bmp",
          ".webp",
          ".heic",
          ".svg",
          ".ai",
          ".eps",
          ".psd",
          ".tiff",
          ".cr2",
          ".nef",
          ".orf",
          ".sr2"
        ]
      }
    },
    {
      "DestinationFolder": "Archives",
      "Conditions": {
        "extensions": [
          ".zip",
          ".rar",
          ".7z",
          ".tar",
          ".gz",
          ".bz2",
          ".iso",
          ".img",
          ".dmg"
        ]
      }
    },
    {
      "DestinationFolder": "Documents",
      "Conditions": {
        "extensions": [
          ".pdf",
          ".docx",
          ".doc",
          ".xlsx",
          ".xls",
          ".pptx",
          ".ppt",
          ".txt",
          ".rtf",
          ".odt",
          ".ods",
          ".md",
          ".csv",
          ".epub",
          ".mobi"
        ]
      }
    },
    {
      "DestinationFolder": "Executables",
      "Conditions": {
        "extensions": [
          ".exe",
          ".msi",
          ".jar"
        ]
      }
    },
    {
      "DestinationFolder": "Scripts",
      "Conditions": {
        "extensions": [
          ".bat",
          ".cmd",
          ".ps1",
          ".sh"
        ]
      }
    },
    {
      "DestinationFolder": "Videos",
      "Conditions": {
        "extensions": [
          ".mp4",
          ".mov",
          ".avi",
          ".mkv",
          ".webm",
          ".wmv",
          ".flv"
        ]
      }
    },
    {
      "DestinationFolder": "Audio",
      "Conditions": {
        "extensions": [
          ".mp3",
          ".wav",
          ".flac",
          ".aac",
          ".ogg",
          ".m4a"
        ]
      }
    },
    {
      "DestinationFolder": "Source Code",
      "Conditions": {
        "extensions": [
          ".html",
          ".css",
          ".js",
          ".ts",
          ".jsx",
          ".tsx",
          ".php",
          ".scss",
          ".less",
          ".vue",
          ".c",
          ".h",
          ".cpp",
          ".hpp",
          ".cs",
          ".csproj",
          ".sln",
          ".vb",
          ".java",
          ".kt",
          ".swift",
          ".py",
          ".go",
          ".rs",
          ".rb",
          ".pl",
          ".sql",
          ".json",
          ".xml",
          ".yml",
          ".yaml",
          ".toml",
          ".dockerfile",
          ".gitignore"
        ]
      }
    }
  ],
  "OthersFolderName": "Others",
  "SubfoldersFolderName": "Folders"
}